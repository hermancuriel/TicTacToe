<Window x:Class="TicTacToe.AiDifficulty"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AiDifficulty" Height="100" Width="150" WindowStartupLocation="CenterScreen">
    
    <StackPanel Name="GuestDifficulty" Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center">
        <Button Content="Easy" Click="Easy_Click" />
        <Button Content="Medium" Click="Medium_Click"/>
        <Button Content="Hard" Click="Hard_Click"/>
    </StackPanel>
</Window>
